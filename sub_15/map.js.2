					content: popupContent,
					disableAutoPan: false,
					maxWidth: 0,
					pixelOffset: new google.maps.Size(-200, -40),
					zIndex: null,
					boxStyle: {
						width: "400px"
					},
					closeBoxMargin: "0 0 0 0",
					closeBoxURL: "about:blank",
					infoBoxClearance: new google.maps.Size(1, 1),
					isHidden: false,
					pane: "floatPane",
					alignBottom: true,
					enableEventPropagation: false

					'</div>' +
					'<div class="meta">' +
					'<div class="popup-location locatie">' + location + '</div>' +
					'<div class="popup-theme theme black ' + mapItem.meta.citydeal_theme.slug + '">' + mapItem.meta.citydeal_theme.name + '</div>' +
					'</div>' +
					'</div>' +
